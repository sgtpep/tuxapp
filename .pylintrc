[FORMAT]
indent-string='  '

[MASTER]
init-hook=import sys; sys.path.append('.')
load-plugins=lib.linting

[MESSAGES CONTROL]
disable=bad-continuation, invalid-name, line-too-long, locally-disabled, missing-docstring, no-self-use, similarities, too-many-boolean-expressions, too-many-lines, unnecessary-lambda, unused-argument, used-before-assignment

[REPORTS]
reports=no
